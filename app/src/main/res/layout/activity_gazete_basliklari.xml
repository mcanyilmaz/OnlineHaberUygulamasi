    android:id="@+id/activity_gazete_basliklari"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.can.yenihaberuygulama.GazeteBasliklari">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:src="@drawable/girisekrani"
        android:id="@+id/gazete_basliklari_resim"/>

    <ListView
        android:layout_below="@+id/gazete_basliklari_resim"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/listem"
        ></ListView>



</RelativeLayout>
